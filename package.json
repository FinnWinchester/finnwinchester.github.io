{
  "name": "finnwinchester-portfolio",
  "description": "ReactJS portfolio",
  "private": true,
  "author": "Finn Winchester",
  "version": "2.0.0",
  "repository": "",
  "scripts": {
    "start": "npm run webpack_dev",
    "start_serv": "webpack-dev-server --content-base docs/",
    "webpack_dev": "webpack --env.development --progress --config webpack.config.babel.js --watch",
    "webpack_prod": "webpack --env.production --progress --config webpack.config.babel.js",
    "build": "npm run webpack_prod",
    "copy_all": "mkdir -p docs && cp -fr ./src/assets/* docs/",
    "libs": "npm install",
    "prebuild": "npm run fresh && npm run libs && npm run copy_all",
    "clean": "rm -fr docs",
    "prestart": "npm run libs && npm run clean && npm run copy_all",
    "prestart_serv": "npm run prestart",
    "fresh": "npm run clean && rm -fr node_modules && rm -fr package-lock.json",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router": "^3.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "~6.22",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.9",
    "enhanced-resolve": "^4.0.0-beta.4",
    "es6-promise-debounce": "^1.0.1",
    "extract-loader": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-replace-webpack-plugin": "^2.2.8",
    "html-string-replace-webpack-plugin": "^1.0.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.3.0",
    "jquery": "^3.2.1",
    "js-md5": "^0.7.2",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "material-ui": "^0.20.0",
    "moment": "^2.19.1",
    "react-transition-group": "^1.2.1",
    "string-replace-loader": "^1.3.0",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2"
  }
}
